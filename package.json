{
  "name": "taskbox",
  "version": "0.1.0",
  "description": "Starter template to get up and running quickly with Angular and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-angular-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-angular-template/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --watchAll",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && storybook dev -p 6006",
    "build-storybook": "npm run docs:json && storybook build",
    "init-msw": "msw init src/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@ngrx/store": "15.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "^15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@babel/core": "^7.18.2",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-a11y": "^7.0.0-beta.15",
    "@storybook/addon-actions": "^7.0.0-beta.15",
    "@storybook/addon-essentials": "^7.0.0-beta.15",
    "@storybook/addon-interactions": "^7.0.0-beta.15",
    "@storybook/addon-links": "^7.0.0-beta.15",
    "@storybook/angular": "^7.0.0-beta.15",
    "@storybook/jest": "^0.0.11-next.0",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.5",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "msw": "^0.49.2",
    "msw-storybook-addon": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.15",
    "typescript": "~4.8.4"
  }
}
